@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    @*<meta http-equiv="X-UA-Compatible" content="IE=edge">*@
    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    <!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
    <title>登录</title>

    <!-- Bootstrap -->
    <link href="@(V.include_css("bootstrap.min.css"))" rel="stylesheet">
    <link href="@(V.include_css("style.css"))" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="//cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div class="login">
        <div class="form-group">
            <label class="control-label">账号</label>
            <input type="text" class="form-control" name="account">
        </div>
        <div class="form-group">
            <label class="control-label">密码</label>
            <input type="password" class="form-control" name="password">
        </div>
        <div class="form-group">
            <button type="button" class="btn btn-info">登&nbsp;&nbsp;&nbsp;&nbsp;录</button>
        </div>
    </div>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="//cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->

    <script>
        var GeneralFrameWork = GeneralFrameWork || {};
        GeneralFrameWork.static_js = '@V.static_js()';
    </script>
    <script src="@(V.include_js("/lib/bootstrap.min.js"))"></script>
    <script src="@(V.include_js("require.js"))"></script>
    <script src="@(V.include_js("config.js"))"></script>
    <script>
        require(["login"], function () {

        });
    </script>
</body>
</html>
